{
  "name": "devebot-co-redis",
  "version": "0.3.2",
  "description": "Devebot - Redis bridge",
  "main": "index.js",
  "files": [
    "bin/",
    "config/",
    "data/",
    "lib/",
    "manifest.js"
  ],
  "scripts": {
    "build": "babel ./src -d ./lib",
    "clean": "rm -rf ./lib",
